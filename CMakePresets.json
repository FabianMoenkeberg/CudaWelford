{
    "version": 3,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 21,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "FirstOne",
            "displayName": "Visual Studio Professional 2019 Release - amd64_x86",
            "description": "Using compilers for Visual Studio 16 2019 (x64_x86 architecture)",
            "generator": "Visual Studio 16 2019",
            "architecture": "win32",
            "binaryDir": "${sourceDir}/build/",
            "cacheVariables": {
                "BUILD_GTEST": "ON",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/build/",
                "CMAKE_VERBOSE_MAKEFILE": "TRUE"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "FirstOne-debug",
            "displayName": "Visual Studio Professional 2019 Release - amd64_x86 - Debug",
            "configurePreset": "FirstOne",
            "configuration": "Debug"
        }
    ],
    "testPresets": [
    {
      "name": "Test_First",
      "configurePreset": "FirstOne",
      "output": {"outputOnFailure": true},
      "execution": {"noTestsAction": "error", "stopOnFailure": false}
    }
  ]
}